<!--TODO this is currently tuned to SshPkgAudit implementation-->
<div class="rat-bubble">
  <div class="rat-part-header">
    <%= participant.current_state %>
    <b><%= participant.name %></b>
    for <%= participant.scope %>
  </div>
  <div class="rat-part">
    <label class="text">Reports</label>
    <pre>
      <%= participant.fields['stdout'] %>
    </pre>
    <label class="text">Errors</label>
    <pre>
      <%= participant.fields['stderr'] %>
    </pre>
  </div>
</div>
